<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="100.0" minWidth="100.0" prefHeight="475.0" prefWidth="666.5" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <TableView fx:id="przedmiotyTable" layoutX="2.0" layoutY="54.0" prefHeight="297.0" prefWidth="666.5">
         <columns>
            <TableColumn fx:id="nameColumn" editable="false" onEditStart="#editGrade" prefWidth="200.0" text="NAZWA PRZEDMIOTU" />
            <TableColumn fx:id="cenaColumn" onEditCommit="#editGrade" prefWidth="70.0" text="CENA" />
            <TableColumn fx:id="iloscColumn" onEditCommit="#editGrade" prefWidth="70.0" text="ILOSC" />
            <TableColumn fx:id="zapotrzebowanieColumn" onEditCommit="#editGrade" prefWidth="165.0" text="ZAPOTRZEBOWANIE" />
            <TableColumn fx:id="rozmiarColumn" onEditCommit="#editGrade" prefWidth="70.0" text="ROZMIAR" />
            <TableColumn fx:id="priorytetColumn" editable="true" onEditStart="#getImage" prefWidth="90.0" text="PRIORYTET" />
         </columns>
      </TableView>
      <Button fx:id="EN" layoutX="25.0" layoutY="2.0" mnemonicParsing="false" onAction="#EN" text="EN" />
      <Button fx:id="PL" layoutX="60.0" layoutY="2.0" mnemonicParsing="false" onAction="#PL" text="PL" />     
      <Button fx:id="ADD" layoutX="100.0" layoutY="414.0" mnemonicParsing="false" onAction="#addNewPrzedmiot" text="DODAJ PRZEDMIOT" />
      <Button fx:id="DELETE" layoutX="245.0" layoutY="414.0" mnemonicParsing="false" onAction="#removePrzedmiot" text="USUN PRZEDMIOT" />
      <TextField fx:id="newPrzedmiotIndex" layoutX="200.25" layoutY="375.0" prefHeight="15.0" prefWidth="250.0" />
      <Button fx:id="ZAPIS" layoutX="407.0" layoutY="414.0" mnemonicParsing="false" onAction="#serializeData" text="ZAPIS" />
      <Button fx:id="ODCZYT" layoutX="492.0" layoutY="414.0" mnemonicParsing="false" onAction="#deserializeData" text="ODCZYT" />
      <Button fx:id="OBRAZ" layoutX="2.0" layoutY="414.0" mnemonicParsing="false" onAction="#viewImage" text="OBRAZ" />
   
   </children>
</AnchorPane>
