            <Label fx:id="MainLabel" text="Label" BorderPane.alignment="CENTER" />
         </top>
         <bottom>
            <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#acknowledgeCaptain" text="Ai captain" BorderPane.alignment="CENTER" />
